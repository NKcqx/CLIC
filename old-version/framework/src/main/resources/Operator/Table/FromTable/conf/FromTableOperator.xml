<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="FromTableOperator" name="FromTableOperator" kind="transformer">

    <platforms>
        <platform ID="spark">
            <path>Operator/Table/FromTable/conf/SparkFromTableOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter name="udfName" data_type="string"></parameter>
    </parameters>

    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"></input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"></output>
    </outputs>
</Operator>
