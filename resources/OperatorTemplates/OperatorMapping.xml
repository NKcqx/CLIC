<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- api的接口内 如sort函数，需要能用特定OperatorTemplates创建出指定的Operator来，因此需要提供一个mapping -->
<mappings>
    <pair>
        <ability>sort</ability>
        <template>/resources/Operator/Sort/conf/SortOperator.xml</template>
    </pair>
    <pair>
        <ability>map</ability>
        <template>/resources/Operator/Map/conf/MapOperator.xml</template>
    </pair>
    <pair>
        <ability>filter</ability>
        <template>/resources/Operator/Filter/conf/FilterOperator.xml</template>
    </pair>
    <pair>
        <ability>collect</ability>
        <template>/resources/Operator/Collect/conf/CollectOperator.xml</template>
    </pair>
</mappings>