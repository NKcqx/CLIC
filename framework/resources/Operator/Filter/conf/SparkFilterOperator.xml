<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="FilterOperator" name="FilterOperator" kind="shuffler">

    <platform ID="spark">
        <language>java</language>
<!--            此处先用jar做image测试-->
        <implementation>/resources/Operator/Filter/SparkFilter.jar</implementation>
        <command>
            java -jar ./resources/Operator/Filter/SparkFilter.jar
            -Dinput_data_path={$input_file_path}
            -Doutput_data_path={$output_file_path}
            -Ddelimiter={$delimiter}
            -Dis_reverse={$is_reverse}
        </command>
<!--            cost是该operator的cost，此处先简单设定-->
        <cost>7.0028</cost>
    </platform>


</Operator>