<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="SqlSinkOperator" name="SqlSinkOperator" kind="consumer">

    <platforms>
        <platform ID="sparkSql">
            <path>Operator/SqlSink/conf/SparkSqlSinkOperator.xml</path>
        </platform>
    </platforms>

    <parameters> <!-- 计算参数 -->
<!--        <parameter name="separator" data_type="string" default=","> </parameter> -->
        <parameter name="outputPath" data_type="string"> </parameter>
        <parameter name="partitionNum" data_type="integer" default="5"> </parameter>
    </parameters>
    <inputs>    <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> </outputs>

</Operator>