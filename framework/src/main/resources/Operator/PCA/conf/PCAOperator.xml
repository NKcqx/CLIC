<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="PCAOperator" name="PCAOperator" kind="consumer">

    <platforms>
        <platform ID="pytorch">
            <path>Operator/PCA/conf/PytorchPCAOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter kind="input" name="k" data_type="string" required="true"> </parameter>
        <parameter kind="input" name="center" data_type="string" default="True"> </parameter>
    </parameters>
    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"> </output>
    </outputs>

</Operator>