<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="ProjectOperator" name="ProjectOperator" kind="supplier">

    <platforms>
        <platform ID="java">
            <path>Source/conf/JavaSourceOperator.xml</path>
        </platform>

        <platform ID="spark">
            <path>Source/conf/SparkSourceOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter name="predicate" data_type="string" required="true"> </parameter>
    </parameters>
    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"> </output>
    </outputs>

</Operator>