<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="StreamWindowOperator" name="StreamWindowOperator" kind="transformer">

    <platforms>
        <platform ID="spark">
            <path>Operator/Stream/StreamWindow/conf/SparkStreamWindowOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter name="winLength" data_type="string" required="true" default="6"> </parameter>
        <parameter name="rollLength" data_type="string" required="true" default="3"> </parameter>
    </parameters>
    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"> </output>
    </outputs>


</Operator>