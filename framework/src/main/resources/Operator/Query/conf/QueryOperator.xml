<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="QueryOperator" name="QueryOperator" kind="transformer">

    <platforms>
        <platform ID="spark">
            <path>Operator/Query/conf/SparkQueryOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter name="sqlText"  data_type="string" required="true"></parameter>
    </parameters>

    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"> </output>
    </outputs>


</Operator>
