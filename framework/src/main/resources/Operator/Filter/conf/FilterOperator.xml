<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="FilterOperator" name="FilterOperator" kind="transformer">

    <platforms>
        <platform ID="java">
            <path>Operator/Filter/conf/JavaFilterOperator.xml</path>
        </platform>

        <platform ID="spark">
            <path>Operator/Filter/conf/SparkFilterOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter name="udfName" data_type="string" required="true"> </parameter>
    </parameters>

    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"> </output>
    </outputs>

</Operator>