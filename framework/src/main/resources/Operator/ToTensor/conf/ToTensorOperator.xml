<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="ToTensorOperator" name="ToTensorOperator" kind="consumer">

    <platforms>
        <platform ID="pytorch">
            <path>Operator/ToTensor/conf/PytorchToTensorOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter kind="input" name="dtype" data_type="string" default="torch.float"> </parameter>
    </parameters>
    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"> </output>
    </outputs>

</Operator>