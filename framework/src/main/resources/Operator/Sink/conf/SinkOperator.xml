<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="SinkOperator" name="SinkOperator" kind="consumer">

    <platforms>
        <platform ID="java">
            <path>Operator/Sink/conf/JavaSinkOperator.xml</path>
        </platform>

        <platform ID="spark">
            <path>Operator/Sink/conf/SparkSinkOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter kind="input" name="output" data_type="string" required="true"> </parameter> <!-- 名字叫output，但它其实是个输入参数 -->
        <parameter kind="input" name="separator" data_type="string" default=","> </parameter> <!--参数默认值怎么做?-->
    </parameters>

</Operator>