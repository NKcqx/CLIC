<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="SqlExeOperator" name="SqlExeOperator" kind="transformer">

    <platforms>
        <platform ID="sparkSql">
            <path>Operator/SqlExe/conf/SparkSqlExeOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <!-- 为什么需要这个参数？因为spark sql需要用table名字给dataframe注册 -->
        <parameter name="tableNames" data_type="string" required="true"></parameter>
        <parameter name="sqlText"  data_type="string" required="true"></parameter>
    </parameters>

    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"> </output>
    </outputs>


</Operator>