<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="TJoinOperator" name="TJoinOperator" kind="shuffler">

    <platforms>
        <platform ID="spark">
            <path>Operator/Table/TJoin/conf/SparkTJoinOperator.xml</path>
        </platform>
    </platforms>

    <parameters>
        <parameter name="schema" data_type="string"></parameter>
        <parameter name="condition" data_type="string"></parameter>
    </parameters>

    <inputs>  <!-- 输入数据 -->
        <input name="leftTable" data_type="string" required="true"></input>
        <input name="rightTable" data_type="string" required="true"></input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"></output>
    </outputs>
</Operator>
