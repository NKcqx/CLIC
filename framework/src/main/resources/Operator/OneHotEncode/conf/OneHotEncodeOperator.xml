<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Operator ID="OneHotEncodeOperator" name="OneHotEncodeOperator" kind="consumer">

    <platforms>
        <platform ID="pytorch">
            <path>Operator/OneHotEncode/conf/PytorchOneHotEncodeOperator.xml</path>
        </platform>
        <platform ID="spark-ml">
            <path>Operator/OneHotEncode/conf/SparkMLOneHotEncodeOperator.xml</path>
        </platform>
    </platforms>


    <inputs>  <!-- 输入数据 -->
        <input name="data" data_type="string" required="true"> </input>
    </inputs>
    <outputs> <!-- 计算结果 -->
        <output name="result" data_type="string"> </output>
    </outputs>

</Operator>