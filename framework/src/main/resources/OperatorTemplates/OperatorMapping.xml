<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- api的接口内 如sort函数，需要能用特定OperatorTemplates创建出指定的Operator来，因此需要提供一个mapping -->
<mappings>
    <pair>
        <ability>source</ability>
        <template>Operator/Source/conf/SourceOperator.xml</template>
    </pair>
    <pair>
        <ability>sink</ability>
        <template>Operator/Sink/conf/SinkOperator.xml</template>
    </pair>
    <pair>
        <ability>sort</ability>
        <template>Operator/Sort/conf/SortOperator.xml</template>
    </pair>
    <pair>
        <ability>map</ability>
        <template>Operator/Map/conf/MapOperator.xml</template>
    </pair>
    <pair>
        <ability>filter</ability>
        <template>Operator/Filter/conf/FilterOperator.xml</template>
    </pair>
    <pair>
        <ability>collect</ability>
        <template>Operator/Collect/conf/CollectOperator.xml</template>
    </pair>
    <pair>
        <ability>project</ability>
        <template>Operator/Project/conf/ProjectOperator.xml</template>
    </pair>
    <pair>
        <ability>join</ability>
        <template>Operator/Join/conf/JoinOperator.xml</template>
    </pair>
    <pair>
        <ability>reduce-by-key</ability>
        <template>Operator/ReduceByKey/conf/ReduceByKeyOperator.xml</template>
    </pair>
    <pair>
        <ability>sqlSource</ability>
        <template>Operator/SqlSource/conf/SqlSourceOperator.xml</template>
    </pair>
    <pair>
        <ability>sqlExe</ability>
        <template>Operator/SqlExe/conf/SqlExeOperator.xml</template>
    </pair>
    <pair>
        <ability>sqlSink</ability>
        <template>Operator/SqlSink/conf/SqlSinkOperator.xml</template>
    </pair>
    <pair>
        <ability>loop</ability>
        <template>Operator/Loop/conf/LoopOperator.xml</template>
    </pair>
    <pair>
        <ability>collection-source</ability>
        <template>Operator/CollectionSource/conf/CollectionSourceOperator.xml</template>
    </pair>
</mappings>